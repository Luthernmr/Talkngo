<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
        <link rel="stylesheet" href="../public/css/style.css">
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
        <header class="large-screen">

            <img class="logo" src="../public/css/img/logo-horizontal (1).svg" alt="">
            <div class="header-content">
    
                <li><input class="searchbar" type="search" name="" placeholder="Chercher un pays"></li>
    
                <li>
                    <div class="spacebar"></div>
                </li>
            
                <li><a class="register" href="">Inscription</a></li>
                <li><a class="login" href="">Connexion</a></li>
    
            </div>
        </header>
    
        <header class="phone-screen">
            <img class="logo" src="../public/css/img/logo-vertical (2).svg" alt="">
            <input class="searchbar" type="search" name="" placeholder="Chercher un pays">
    
            <a class="register" href="">Inscription</a>
            <a class="login" href="">Connexion</a>
    
        </header>
    
        {% block body %}<body>

            <a href="{{ path('admin_user') }}">users</a>   
                   
        </body>{% endblock %}
        <footer>
            <li><a href="#">A propos</a></li>
            <li><a href="#">Nous contacter</a></li>
            <li><a href="#">Talk'n Go Â© Copyright 2021</a></li>
        </footer>  
    </body>
</html>
